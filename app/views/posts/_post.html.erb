<div id="<%= dom_id post %>">

  <% if post.status != false %>
  <p>
    <strong>Title:</strong>
    <%= post.title %>
  </p>

  <p>
    <strong>Content:</strong>
    <%= post.content %>
  </p>

  <p>
    <strong>Category:</strong>
    <%= post.category.title %>
  </p>
  <p>
    <strong>Created by:</strong>
    <%= post.user.email %>
  </p>

  <% end %>
 
</div>
